{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}